{
  "name": "ai-coding-agent-backend",
  "version": "1.0.0",
  "description": "Backend for Claude-style AI Coding Agent with Gemini 2.5 API",
  "main": "dist/server.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "build": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js --external:@prisma/client --external:better-sqlite3 --external:esbuild",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/seed.ts",
    "db:studio": "prisma studio",
    "type-check": "tsc --noEmit",
    "export-examples": "tsx export-example.ts"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "gemini",
    "langchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@google/generative-ai": "^0.21.0",
    "@langchain/community": "^0.3.16",
    "@langchain/core": "^0.3.24",
    "@langchain/google-genai": "^0.1.3",
    "@prisma/client": "^6.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esbuild": "^0.24.2",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.8",
    "lru-cache": "^11.1.0",
    "morgan": "^1.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^11.0.4",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
